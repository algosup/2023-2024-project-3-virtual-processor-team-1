.hello
    MOV R2, "Hello, World!"
    DISP R2
;RET

.function
    MOV R1, "Hello you"
    CALL .hello
    DISP R1
END

CALL .function

MOV R1, -5
DISP R1     ; expected 5
MOV R2, #7   ; Test comment

DISP R2     ; expected 7
ADD R1, R2
DISP R1     ; expected 12
SUB R2, #3
DISP R2     ; 4

DISP "Hello World" ; expected Hello World

; expected --> R1 = 5, R2 = 7, R1 = 12, R2 = 4

DISP "End of program"